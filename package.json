{
  "name": "starter_template_fe",
  "version": "1.0.0-alpha",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "node --max_old_space_size=4096 ./node_modules/vite/bin/vite.js build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "npm run test -- --ui",
    "typecheck": "tsc",
    "lint": "eslint . --fix",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier . --check",
    "format:fix": "npm run format -- --write",
    "prepare": "husky install",
    "test:staged": "cross-env CI=true craco test --findRelatedTests",
    "eject": "craco eject",
    "sync-sort-translations": "node src/utils/translations/syncSortTranslations.js",
    "validate-translations": "node src/utils/translations/translationsValidator.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "generate-api": "orval --config ./orval.config.ts"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(react-syntax-highlighter)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "resetMocks": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version",
      "IE 11"
    ]
  },
